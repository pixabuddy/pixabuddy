{
	"name": "pixabuddy",
	"version": "1.0.0",
	"description": "Pixabuddy official site",
	"main": "index.js",
	"scripts": {
		"watch:sass": "node-sass assets/sass/main.scss assets/css/style.css -w",
		"devserver": "live-server",
		"start": "npm-run-all --parallel devserver watch:sass",
		"compile:sass": "node-sass assets/sass/main.scss assets/css/style.comp.css",
		"compress:css": "node-sass  assets/css/style.comp.css assets/css/style.css --output-style compressed",
		"build:css": "npm-run-all compile:sass compress:css"
	},
	"author": "Abhishek",
	"license": "ISC",
	"devDependencies": {
		"autoprefixer": "^10.1.0",
		"concat": "^1.0.3",
		"node-sass": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"postcss-cli": "^8.3.1"
	}
}
